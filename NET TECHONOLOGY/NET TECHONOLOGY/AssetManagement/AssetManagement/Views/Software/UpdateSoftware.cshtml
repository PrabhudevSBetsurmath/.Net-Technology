@model AssetManagement.Models.Software

@{
    ViewData["Title"] = "UpdateSoftware";
}

<h1>UpdateSoftware</h1>

<h4>Software</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="UpdateSoftware">
            @Html.ValidationSummary(true)
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="id" class="control-label"></label>
                <input asp-for="id" class="form-control" />
                <span asp-validation-for="id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="name" class="control-label"></label>
                <input asp-for="name" class="form-control" />
                <span asp-validation-for="name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="dateOfPublish" class="control-label"></label>
                <input asp-for="dateOfPublish" class="form-control" />
                <span asp-validation-for="dateOfPublish" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="dateOfExpiry" class="control-label"></label>
                <input asp-for="dateOfExpiry" class="form-control" />
                <span asp-validation-for="dateOfExpiry" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="price" class="control-label"></label>
                <input asp-for="price" class="form-control" />
                <span asp-validation-for="price" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

